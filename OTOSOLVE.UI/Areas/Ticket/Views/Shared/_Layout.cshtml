<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <!-- App favicon -->
    <link rel="shortcut icon" href="~/matrica/assets/images/favicon.ico">

    <!-- App css -->
    <link href="~/matrica/assets/css/bootstrap-material.min.css" rel="stylesheet" type="text/css">
    <link href="~/matrica/assets/css/icons.min.css" rel="stylesheet" type="text/css">
    <link href="~/matrica/assets/css/app-material.min.css" rel="stylesheet" type="text/css">
    <link href="~/matrica/assets/css/app.min.css" rel="stylesheet" type="text/css" />
    <link href="~/matrica/assets/css/app-font.css" rel="stylesheet" type="text/css" />
    <link href="~/matrica/assets/css/app-custom.css" rel="stylesheet" type="text/css" />
    <link href="~/matrica/assets/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" type="text/css">
    <link href="~/matrica/assets/libs/animate.css/animate.min.css" rel="stylesheet" type="text/css">

    <title>::OtoSolve - Ticket System::</title>
    @if (IsSectionDefined("Styles"))
    {
        @await RenderSectionAsync("Styles", true)
    }
    else
    {
        @await RenderSectionAsync("Styles", false)
    }
</head>
<body id="body" class="menuitem-active">

    <!-- Left Side-Bar Start -->
    <div class="left-sidebar show">

        <!-- LOGO -->
        <div class="brand">
            <a asp-area="" asp-controller="Login" asp-action="Login" class="logo">
                <span>
                    <img src="~/matrica/assets/images/logo-sm.png" alt="logo-small" class="logo-sm">
                </span>
                <span>
                    <img src="~/matrica/assets/images/logo.png" alt="logo-large" class="logo-lg logo-light">
                    <img src="~/matrica/assets/images/logo-dark.png" alt="logo-large" class="logo-lg logo-dark">
                </span>
            </a>
        </div><!--end logo-->
        <!-- Menu -->
        <div class="menu-content h-100" data-simplebar="init">
            <div class="simplebar-wrapper" style="margin: 0px 0px -60px;">

                <div class="simplebar-height-auto-observer-wrapper">
                    <div class="simplebar-height-auto-observer"></div>
                </div>

                <div class="simplebar-mask">
                    <div class="simplebar-offset" style="right: -16.6667px; bottom: 0px;">
                        <div class="simplebar-content-wrapper" style="height: 100%; overflow: hidden scroll;">
                            <div class="simplebar-content" style="padding: 0px 0px 60px;">
                                <div class="menu-body navbar-vertical tab-content menuitem-active">
                                    <div class="collapse navbar-collapse show" id="sidebarCollapse">

                                        <!-- Navigation -->
                                        <ul class="navbar-nav">
                                            <li class="menu-label mt-0">T<span>icket System</span></li>

                                            <li class="nav-item menuitem-active">
                                                <a class="nav-link collapsed" href="#sidebarDashboards" data-bs-toggle="collapse" role="button" aria-expanded="false" aria-controls="sidebarAnalytics">
                                                    <i class="ti ti-home menu-icon"></i>
                                                    <span>Dashboards</span>
                                                </a>
                                                <div class="collapse" id="sidebarDashboards">
                                                    <ul class="nav flex-column">
                                                        <li class="nav-item menuitem-active">
                                                            <a class="nav-link active" href="#" aria-expanded="true">Analytics</a>
                                                        </li><!--end nav-item-->
                                                        <li class="nav-item">
                                                            <a class="nav-link" asp-area="Ticket" asp-controller="Ticket" asp-action="TPCreate"> Process</a>
                                                        </li><!--end nav-item-->
                                                         <li class="nav-item">
                                                            <a class="nav-link" asp-area="Ticket" asp-controller="Ticket" asp-action="TMWorkFlowRule"> Workflow Rule</a>
                                                        </li><!--end nav-item-->
                                                        <li class="nav-item">
                                                            <a class="nav-link" asp-area="Ticket" asp-controller="Ticket" asp-action="TCreate"> Ticket</a>
                                                        </li><!--end nav-item-->
                                                        <li class="nav-item">
                                                            <a class="nav-link" asp-area="Ticket" asp-controller="Ticket" asp-action="TPView">Process Tickets</a>
                                                        </li><!--end nav-item-->
                                                        <li class="nav-item">
                                                            <a class="nav-link" asp-area="Ticket" asp-controller="Ticket" asp-action="WFView">Workflow Tickets</a>
                                                        </li><!--end nav-item-->
                                                        <li class="nav-item">
                                                            <a class="nav-link" asp-area="Ticket" asp-controller="Ticket" asp-action="WFView">Workflow Tickets</a>
                                                        </li><!--end nav-item-->
                                                        <li class="nav-item">
                                                            <a class="nav-link" href="#">Helpdesk</a>
                                                        </li><!--end nav-item-->
                                                        <li class="nav-item">
                                                            <a class="nav-link" href="#">Setting</a>
                                                        </li><!--end nav-item-->
                                                    </ul><!--end nav-->
                                                </div><!--end sidebarDashboards-->
                                            </li><!--end nav-item-->

                                            <li class="menu-label mt-0">C<span>onfiguration</span></li>
                                            <li class="nav-item">
                                                <a class="nav-link" href="#sidebarAnalytics" data-bs-toggle="collapse" role="button" aria-expanded="true" aria-controls="sidebarAnalytics">
                                                    <i class="ti ti-stack menu-icon"></i>
                                                    <span>Settings</span>
                                                </a>
                                                <div class="collapse show" id="sidebarAnalytics">
                                                    <ul class="nav flex-column">
                                                        <li class="nav-item">
                                                            <a href="#" class="nav-link ">Customers</a>
                                                        </li><!--end nav-item-->
                                                        <li class="nav-item">
                                                            <a href="#" class="nav-link ">Administration</a>
                                                        </li><!--end nav-item-->
                                                    </ul><!--end nav-->
                                                </div><!--end sidebarAnalytics-->
                                            </li><!--end nav-item-->

                                        </ul><!--end navbar-nav--->

                                    </div>
                                </div><!--endmenu-body--->
                            </div>
                        </div>
                    </div>

                </div><!-- end simplebar-mask-->
            </div><!-- end simplebar-wrapper-->
        </div><!-- end menu-->
    </div><!-- end left-sidenav-->
    <!-- Top Bar Start -->
    <div class="topbar">
        <nav class="navbar-custom nav-sticky" id="navbar-custom">
            <ul class="list-unstyled topbar-nav float-end mb-0">
                <li class="dropdown">
                    <a class="nav-link dropdown-toggle nav-user" data-bs-toggle="dropdown" href="#" role="button" aria-haspopup="false" aria-expanded="false">
                        <div class="d-flex align-items-center">
                            <img src="~/matrica/assets/images/person.png" alt="profile-user" class="rounded-circle me-2 thumb-sm">
                            <div>
                                <small class="d-none d-md-block font-11">Admin</small>
                                <span class="d-none d-md-block fw-semibold font-12">Maria Gibson <i class="mdi mdi-chevron-down"></i></span>
                            </div>
                        </div>
                    </a>
                    <div class="dropdown-menu dropdown-menu-end">
                        <a class="dropdown-item" href="#"><i class="ti ti-user font-16 me-1 align-text-bottom"></i> Profile</a>
                        <a class="dropdown-item" href="#"><i class="ti ti-settings font-16 me-1 align-text-bottom"></i> Settings</a>
                        <div class="dropdown-divider mb-0"></div>
                        <a class="dropdown-item" href="#"><i class="ti ti-power font-16 me-1 align-text-bottom"></i> Logout</a>
                    </div>
                </li><!--end topbar-profile-->
            </ul><!--end topbar-navbar-item-->
        </nav><!--end navbar-->
    </div><!--end top bar-->
    <!-- Page Start -->
    <div class="page-wrapper">
        <!-- Page Content-->
        <div class="page-content-tab">
            <div class="container-fluid">
                <!-- Page-Title -->
                <div class="row">
                    <div class="col-sm-12">
                        <div class="page-title-box">
                            <div class="float-end">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item">
                                        <a asp-area="@ViewData["AreaName"]" asp-controller="@ViewData["ControllerName"]" asp-action="@ViewData["ActionName"]">@ViewData["bGParent"]</a>
                                    </li><!--end nav-item-->
                                    <li class="breadcrumb-item">
                                        <a asp-area="@ViewData["AreaName"]" asp-controller="@ViewData["ControllerName"]" asp-action="@ViewData["ActionName"]">@ViewData["bParent"]</a>
                                    </li><!--end nav-item-->
                                    <li class="breadcrumb-item active">@ViewData["bChild"]</li>
                                </ol>
                            </div><!--End Breadcrumb-->
                            <h4 class="page-title">@ViewData["pTitle"]</h4>
                        </div><!--end page-title-box-->
                    </div><!--end col-->
                </div><!-- end page title -->
                <!--Start Content-->
                @* <div id="divBlocker" class="d-none">
					<div id="ldSpinner" class="spinner-border text-primary customspinner" role="status"
						 style=""></div>
				</div> *@
                @RenderBody()
                <!--end Cotent-->
            </div><!-- container -->
            <!-- Footer Start -->
            <footer class="footer text-center text-sm-start">
                Office Management System &copy;
                <script>
                    document.write(new Date().getFullYear())
                </script>
                Urben Techies <span class="d-none d-sm-inline-block float-end"> Crafted with <i class="mdi mdi-heart text-danger"></i> by Aviv Solutions Inc </span>
            </footer><!-- End Footer -->
        </div>
        <!-- end page content -->
    </div>
    <!-- Footer Start -->
    @* <footer class="footer text-center text-sm-start">
        © <script>
              document.write(new Date().getFullYear())
        </script>2025 OtoSolve <span class="text-muted d-none d-sm-inline-block float-end">Crafted with <i class="mdi mdi-heart text-warning"></i> by Aviv Solutions Inc</span>
    </footer> *@
    <!-- end Footer -->
    <!--Script-->
    <!-- vendor js -->
    <script src="~/matrica/assets/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/matrica/assets/libs/feather-icons/feather.min.js"></script>
    <!-- App js -->
    <script src="~/matrica/assets/js/app.js"></script>
    <script src="~/matrica/assets/libs/sweetalert2/sweetalert2.min.js"></script>
    <script src="~/matrica/assets/js/pages/sweet-alert.init.js"></script>

    <!-- App js -->
    <script src="~/matrica/assets/js/app.js"></script>
    @if (IsSectionDefined("Scripts"))
    {
        @await RenderSectionAsync("Scripts", true)
    }
    else
    {
        @await RenderSectionAsync("Scripts", false)
    }

</body>
</html>